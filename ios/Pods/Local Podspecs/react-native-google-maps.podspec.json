{
  "name": "react-native-google-maps",
  "version": "1.3.2",
  "summary": "React Native Mapview component for iOS + Android",
  "authors": "Leland Richardson <leland.m.richardson@gmail.com>",
  "homepage": "https://github.com/react-native-maps/react-native-maps#readme",
  "license": "MIT",
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/react-native-maps/react-native-maps.git",
    "tag": "v1.3.2"
  },
  "source_files": "ios/AirGoogleMaps/**/*.{h,m}",
  "compiler_flags": [
    "-DHAVE_GOOGLE_MAPS=1",
    "-DHAVE_GOOGLE_MAPS_UTILS=1",
    "-fno-modules"
  ],
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "$(inherited) '$(PODS_ROOT)/Google-Maps-iOS-Utils/src/Heatmap' '$(PODS_ROOT)/Headers/Public/'"
  },
  "dependencies": {
    "React-Core": [

    ],
    "GoogleMaps": [
      "7.0.0"
    ],
    "Google-Maps-iOS-Utils": [
      "4.1.0"
    ]
  }
}
